{
  "name": "maf-http",
  "version": "0.7.0",
  "description": "http handling",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf package coverage .nyc_output npm-debug.log",
    "build": "./node_modules/.bin/babel src --out-dir ./package --presets es2015 --source-maps",
    "build:for-test": "npm run build -- --plugins istanbul",
    "build:package-json": "node ./build/copyPackageJson",
    "dev": "npm run build:for-test -- --watch",
    "test": "npm run build:for-test && ./node_modules/.bin/tap test/unit -J --coverage",
    "test:fast": "./node_modules/.bin/tap test/unit -J",
    "test:cov": "npm test -- --coverage-report=html",
    "postversion": "git push origin && git push --tags",
    "prepublish": "in-publish && echo 'use \"npm run _publish\"' && exit 1 || not-in-publish",
    "_publish": "npm run clean && npm run build && npm run build:package-json && cd ./package && npm publish",
    "pu": "npm run _publish",
    "toc": "node ./build/docToc.js"
  },
  "engines": {
    "node": ">=4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mafjs/http.git"
  },
  "keywords": [
    "maf"
  ],
  "author": "alekzonder <alekzonder@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mafjs/http/issues"
  },
  "homepage": "https://github.com/mafjs/http#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "^1.18.1",
    "cookie-parser": "^1.4.3",
    "eslint": "^4.6.1",
    "express": "^4.15.4",
    "in-publish": "^2.0.0",
    "log4js-nested": "^1.0.1",
    "markdown-toc": "^1.1.0",
    "proxyquire": "^1.8.0",
    "source-map-support": "^0.4.18",
    "tap": "^10.7.2"
  },
  "dependencies": {
    "joi": "^11.0.1",
    "lodash.get": "^4.4.2",
    "lodash.has": "^4.5.2",
    "lodash.pick": "^4.4.0",
    "maf-error": "^1.2.0",
    "maf-kind-of": "^0.1.0",
    "uuid": "^3.1.0"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/*.js",
      "src/methods/**/*.js"
    ]
  }
}
